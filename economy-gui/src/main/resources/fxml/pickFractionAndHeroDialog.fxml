<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <VBox alignment="CENTER" spacing="5.0">
               <children>
                  <Label text="GRACZ I" />
                  <ComboBox fx:id="fractionPlayer1ComboBox" prefWidth="150.0" />
                  <ComboBox fx:id="heroPlayer1ComboBox" prefWidth="150.0" />
               </children>
            </VBox>
            <VBox alignment="CENTER" spacing="5.0">
               <children>
                  <Label text="GRACZ II" />
                  <ComboBox fx:id="fractionPlayer2ComboBox" prefWidth="150.0" />
                  <ComboBox fx:id="heroPlayer2ComboBox" prefWidth="150.0" />
               </children>
            </VBox>
         </children>
      </HBox>
      <Button fx:id="beginButton" mnemonicParsing="false" prefWidth="200.0" text="BEGIN" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
